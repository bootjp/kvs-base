node2: ./kvs-infrastructure --raft_id=nodeB --address=localhost:50052 --raft_data_dir /tmp/my-raft-cluster
node3: ./kvs-infrastructure --raft_id=nodeC --address=localhost:50053 --raft_data_dir /tmp/my-raft-cluster

